{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      ".",
      "./src",
      "./src/utils"
    ],
    "exclude": [
      "./node_modules",
      "./tests"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [0--
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./JSDocs/",
    "recurse": true
  }
}