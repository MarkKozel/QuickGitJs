{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src/StatusContainer.js",
      "./src/BranchListContainer.js",
      "./src/RepoContainer.js",
      "./src/WorkingContainer.js",
      "./src/utils/fileUtils.js",
      "./src/utils/gitUtils.js"
    ],
    "exclude": [
      "./node_modules",
      "./tests"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./JSDocs/",
    "recurse": true
  }
}